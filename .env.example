
# Environment Variables Configuration
# Copy this file to .env and fill in the required values

# AWS Configuration
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=
AWS_ACCOUNT_ID=

# GitHub Repository
GITHUB_REPO_URL=

# EC2 Configuration
EC2_KEY_PAIR_NAME=
EC2_INSTANCE_TYPE=
EC2_AMI_ID=ami-xxxxxxxxxxxxxxx

# Network Configuration (Fill these with your actual VPC/Subnet IDs)
VPC_ID=vpc-xxxxxxxxxxxxxxxxxx
SUBNET_ID_1=subnet-xxxxxxxxxxxx
SUBNET_ID_2=subnet-xxxxxxxxxxxxx
SECURITY_GROUP_ID=sg-xxxxxxxxxxxxx

# Application Configuration
APP_NAME=java-app
APP_PORT=9000
JAR_PATH=build/libs/project.jar

# Docker Configuration
DOCKER_IMAGE_NAME=java-app
ECR_REPOSITORY_NAME=java-app-repo

# Load Balancer Configuration
ALB_NAME=java-app-alb
TARGET_GROUP_NAME=java-app-targets
